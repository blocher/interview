{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "algolia/algoliasearch-client-php": "^1.23",
        "aws/aws-php-sns-message-validator": "^1.4",
        "aws/aws-sdk-php": "^3.36",
        "doctrine/annotations": "^1.5",
        "doctrine/dbal": "^2.6",
        "egeloen/http-adapter": "^1.0",
        "fideloper/proxy": "~3.3",
        "fluent/logger": "^1.0",
        "fzaninotto/faker": "^1.7",
        "graylog2/gelf-php": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "hoa/bench": "3.17.01.11",
        "hoa/console": "^3.17",
        "hoa/dispatcher": "^1.17",
        "hoa/router": "3.17.01.14",
        "ircmaxell/random-lib": "^1.2",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravel/passport": "^4.0",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "moontoast/math": "^1.1",
        "nexmo/client": "^1.1",
        "packfire/php5.3-compat": "^1.0",
        "pda/pheanstalk": "^3.1",
        "php-amqplib/php-amqplib": "^2.7",
        "php-console/php-console": "^3.1",
        "php-http/httplug": "^1.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "ramsey/uuid-console": "^1.1",
        "ramsey/uuid-doctrine": "^1.4",
        "rollbar/rollbar": "^1.3",
        "ruflin/elastica": "^5.3",
        "sentry/sentry": "^1.7",
        "spatie/flysystem-dropbox": "^1.0",
        "srmklive/flysystem-dropbox-v2": "^1.0",
        "symfony/browser-kit": "^3.3",
        "symfony/class-loader": "^3.3",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/dom-crawler": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/filesystem": "^3.3",
        "symfony/proxy-manager-bridge": "^3.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
